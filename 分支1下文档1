	<script type="text/javascript" src="../js/jquery-1.11.3.min.js"></script>
	<script>
	// 人物描述小块的颜色
		var inf_sj = document.getElementById("inf-sj").getElementsByTagName("div");
		var infcolor_sj = 900000;
		for (var i = 0; i < inf_sj.length; i++) {
		 	inf_sj[i].style.backgroundColor = "#" + (infcolor_sj - 27371*i);	 	
		 } 
		 // 小头像鼠标移入事件
		$(".smallpic-sj img").mouseover(function() {
			$(this).stop(true).animate({"height" : "182px"},10).siblings().stop(true).animate({"height" : "162px"}, 10).parent().siblings(".bigpic-sj").children().eq($(this).index()).addClass("show-sj").siblings().removeClass("show-sj").parent().siblings(".inf-sj").children().eq($(this).index()).css({"display" : "block"}).siblings().css({"display" :"none"});
			for (var i = $(this).index(); i >= 0; i--) {
				$(".smallpic-sj img").eq(i).css({"z-index" : 19+i-$(this).index()});
				if (i == $(this).index() - 1) {
					$(".smallpic-sj img").eq(i).stop(true).animate({"height" : "175px"},10);
				}
				if (i == $(this).index() - 2) {
					$(".smallpic-sj img").eq(i).stop(true).animate({"height" : "167px"},10);
				}
			}
			for (var i = $(this).index()+1; i < 19; i++) {
				$(".smallpic-sj img").eq(i).css({"z-index" : 19+$(this).index()-i});
				if (i == $(this).index() + 1) {
					$(".smallpic-sj img").eq(i).stop(true).animate({"height" : "175px"},10);
				}
				if (i == $(this).index() + 2) {
					$(".smallpic-sj img").eq(i).stop(true).animate({"height" : "167px"},10);
				}
			}
			

		})	
		// 荣誉墙的变大变小处理
		$(".honor-sj").click(function() {
			$(".mark-sj").css({"display" : "block"}).siblings(".honorbig-sj").fadeIn(1000).children("span").css({"display" : "block"});
		});
		$(".honorbig-sj").click(function() {
			$(".mark-sj").css({"display" : "none"}).siblings(".honorbig-sj").css({"display" : "none"}).children("span").css({"display" : "none"})
		})
		// 资质证书的变大变小处理
		$(".certifi-sj").click(function() {
			$(".mark-sj").css({"display" : "block"}).siblings(".certifibig-sj").fadeIn(1000).children("span").css({"display" : "block"});
		});
		$(".certifibig-sj").click(function() {
			$(".mark-sj").css({"display" : "none"}).siblings(".certifibig-sj").css({"display" : "none"}).children("span").css({"display" : "none"});
		})
	</script>
</html>
